<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<fo:root language="IT" xmlns:fo="http://www.w3.org/1999/XSL/Format">	 
	<fo:layout-master-set> 
        <fo:simple-page-master master-name="A4-portrail" page-height="297mm" page-width="210mm" margin-top="5mm" margin-bottom="5mm" margin-left="5mm" margin-right="5mm">		
            <fo:region-body margin-top="2mm" margin-bottom="2mm"/>
            <fo:region-before region-name="xsl-region-before" extent="25mm" display-align="before" precedence="true"/>
			<fo:region-after region-name="xsl-region-after" extent="25mm" display-align="before" precedence="true"/>
        </fo:simple-page-master>		
    </fo:layout-master-set>	
	
    <fo:page-sequence master-reference="A4-portrail">	
	
	<fo:static-content flow-name="xsl-region-after">
<fo:block space-before="3mm"/>
   <fo:block font-size="9pt" font-weight="normal" font-family="Helvetica" padding-top="16mm"
                margin="0mm 10.4mm 0mm 0mm">
                <fo:block>Page <fo:page-number/> of <fo:page-number-citation 
ref-id="TheVeryLastPage"/></fo:block>
            </fo:block>
            
        </fo:static-content>	
	
	<fo:flow flow-name="xsl-region-body" border-collapse="collapse" reference-orientation="0">	
		
	<fo:block space-before="4mm"/>
	
	<fo:table width="100%"  font-family="sans-serif" table-layout="fixed" space-before="10pt" space-after="10pt" background-color="#C0C0C0" >
		<fo:table-column column-number="1" column-width="110mm"/>
		<fo:table-column column-number="2" column-width="85mm"/>
		<fo:table-body>
			<fo:table-row line-height="3.5mm">
				<fo:table-cell border-style="none" >
					<fo:block text-align="left" line-height="13mm" font-size="15pt" font-weight="bold" margin-left="3mm">Payments Report</fo:block>
				</fo:table-cell>
				<fo:table-cell text-align="right"  line-height="3mm" margin-top="4mm">
					<fo:block font-size="8.5pt" margin-top="3mm">${dateStart} - ${dateEnd}</fo:block>
					<fo:block font-size="8.5pt">Grouped by: Payment Type</fo:block>
				</fo:table-cell>
			</fo:table-row>
		</fo:table-body>
	</fo:table>

	<fo:block text-indent="1em" font-family="sans-serif" font-size="13pt" font-weight="bold" background-color="#C0C0C0" line-height="8mm" border-color="black" border-width="0.50mm" border-style="solid">
		<fo:block>City of Provo</fo:block>
	</fo:block>
		
	<fo:block space-before="2mm"/>