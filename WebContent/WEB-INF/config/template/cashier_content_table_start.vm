					<fo:block text-align="left" font-size="8pt" padding-top="10px" >Amount</fo:block>
				</fo:table-cell>
			</fo:table-row>
		</fo:table-body>
	</fo:table>			
	
	<fo:table font-family="sans-serif" table-layout="fixed" space-before="10pt" space-after="10pt" border-color="black" border-width="0.35mm" border-style="solid">
		<fo:table-column column-number="1" column-width="12mm"/>
		<fo:table-column column-number="2" column-width="22mm"/>
		<fo:table-column column-number="3" column-width="65mm"/>
