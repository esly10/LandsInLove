				</fo:table-cell>
				<fo:table-cell border-style="none" padding-top="20px">
					<fo:block text-align="left" font-size="8pt" padding-top="10px">Violation</fo:block>
				</fo:table-cell>
				<fo:table-cell border-style="none"  padding-top="20px">
					<fo:block text-align="left" font-size="8pt" >Payment Type</fo:block>
					<fo:block text-align="left" font-size="8pt" >Payment Mode</fo:block>
				</fo:table-cell>

				<fo:table-cell border-style="none"  padding-top="20px">
					<fo:block text-align="left" font-size="8pt" >Payment Num</fo:block>
					<fo:block text-align="left" font-size="8pt" >Trans ID</fo:block>
				</fo:table-cell>

				<fo:table-cell border-style="none" padding-top="20px">
					<fo:block text-align="left" font-size="8pt">Payment Day</fo:block>
					<fo:block text-align="left" font-size="8pt" >Received By</fo:block>
