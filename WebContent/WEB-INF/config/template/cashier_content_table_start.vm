		<fo:table-column column-number="5" column-width="28mm"/>

		<fo:table-column column-number="6" column-width="33mm"/>
		<fo:table-column column-number="7" column-width="18mm"/>

		<fo:table-body>
			<fo:table-row border-width="0mm">
				<fo:table-cell border-style="none" padding-top="20px">					
					<fo:block text-align="left" font-size="7pt" padding-top="10px">Payment</fo:block>
				</fo:table-cell>
				<fo:table-cell border-style="none" padding-top="20px">
